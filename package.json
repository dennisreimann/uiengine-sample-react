{
  "private": true,
  "name": "uiengine-sample-react",
  "description": "UIengine sample project using React",
  "author": "Dennis Reimann <mail@dennisreimann.de> (https://dennisreimann.de)",
  "version": "1.0.0",
  "license": "MIT",
  "homepage": "https://github.com/dennisreimann/uiengine",
  "bugs": "https://github.com/dennisreimann/uiengine-sample-react",
  "repository": "dennisreimann/uiengine-sample-react",
  "main": "pages/index.js",
  "scripts": {
    "start": "rimraf dist && npm run start:next & (sleep 5 && npm run start:uiengine) & wait",
    "start:next": "node server.js",
    "start:uiengine": "uiengine build -s -w",
    "build": "rimraf dist && npm run build:next & npm run build:uiengine",
    "build:next": "rimraf out && npm run start:next & (sleep 5 && next build && next export && kill $!)",
    "build:uiengine": "uiengine build",
    "sketch": "rimraf sketch && html-sketchapp --config html-sketchapp.config.js && open -a Sketch"
  },
  "keywords": [
    "uiengine",
    "nextjs",
    "react"
  ],
  "dependencies": {
    "@zeit/next-css": "^0.2.0",
    "classnames": "^2.2.6",
    "express": "^4.16.3",
    "isomorphic-unfetch": "^2.0.0",
    "next": "^6.0.3",
    "prop-types": "^15.6.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.42",
    "@babel/preset-env": "7.0.0-beta.42",
    "@babel/preset-react": "7.0.0-beta.51",
    "@babel/preset-stage-2": "7.0.0-beta.42",
    "@babel/register": "7.0.0-beta.42",
    "@uiengine/adapter-html": "^0.21.0",
    "@uiengine/core": "^0.21.2",
    "ajv": "^6.5.1",
    "babel-plugin-css-modules-transform": "^1.6.1",
    "html-sketchapp-cli": "0.5.1",
    "postcss": "^6.0.22",
    "rimraf": "^2.6.2"
  }
}
